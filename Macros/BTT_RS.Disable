## -- Runout Sensor Settings

## -- Sensor Settings 
[filament_motion_sensor filament_sensor]
detection_length: 10
extruder: extruder
switch_pin: ^P1.24
pause_on_runout: True

runout_gcode:
    PAUSE

## -- Disable Sensor On Startup
[delayed_gcode DISABLEFILAMENTSENSOR]   
initial_duration: 1
gcode:
    SET_FILAMENT_SENSOR SENSOR=filament_sensor ENABLE=0
## -//-